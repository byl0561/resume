<div class="avoid_pagebreak animated fadeInUp">
  <h4 class="title"><i class="fi-{{ module.header.icon }}"></i> {{ module.header.title }}</h4>
  <div class="row experience">
    {% for item in module.header.experience %}
    <div class="large-6 medium-6 small-12 columns {% if loop.last %}end{% endif %} {% if item.animation %}animated {{ item.animation }}{% endif %}">
      <div class="year">{{ 'now'|date('Y') - item.start_year + 1 }}</div>
      <div class="exp_data">
        <div class="type">å¹´{{ item.type }}</div>
        <div class="role">{{ item.highlight1 }}</div>
        <div class="company">{{ item.highlight2 }}</div>
      </div>
    </div>
    {% endfor %}
  </div>
  {{ module.content }}
</div>
